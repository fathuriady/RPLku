<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rpl.rplku"
    >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/logoa"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logoa"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LresepActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".TambahDataActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".TentangActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".LihatData"
            android:screenOrientation="portrait"/>
    </application>

</manifest>